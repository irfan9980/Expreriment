<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Salary</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(Salary__c &gt;=10000 &amp;&amp; Salary__c &lt;=50000)</errorConditionFormula>
    <errorDisplayField>Salary__c</errorDisplayField>
    <errorMessage>Salary should be in the range 10000 and 50000</errorMessage>
</ValidationRule>
